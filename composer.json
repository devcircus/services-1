{
    "name": "perfect-oblivion/services",
    "description": "Service class Implementation for Laravel Projects",
    "version": "1.0.0",
    "keywords": [
        "perfect-oblivion",
        "servicehandler",
        "service",
        "services",
        "domain"
    ],
    "homepage": "https://github.com/perfect-oblivion/services",
    "license": "MIT",
    "authors": [
        {
            "name": "Clayton Stone",
            "email": "clay@perfectoblivion.com",
            "homepage": "https://perfectoblivion.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.7.* || 5.8.*",
        "perfect-oblivion/common": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "PerfectOblivion\\Services\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PerfectOblivion\\Services\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PerfectOblivion\\Services\\ServicesServiceProvider"
            ]
        }
    }
}
